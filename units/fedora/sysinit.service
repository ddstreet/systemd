[Unit]
Description=System Initialization
Before=local-fs.target remote-fs.target swap.target sockets.target

[Service]
ExecStart=/etc/rc.d/rc.sysinit
Type=forking
TimeoutSec=0
StandardInput=tty
ValidNoProcess=yes
