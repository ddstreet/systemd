[Unit]
Description=System Initialization
DefaultDependencies=no
Conflicts=shutdown.target
Before=shutdown.target emergency.service emergency.target

[Service]
ExecStart=/etc/rc.d/rc.sysinit
Type=forking
TimeoutSec=0
StandardInput=tty
RemainAfterExit=yes
